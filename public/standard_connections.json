          },
          "right" : {
            "tableAlias" : "Albums",
            "columnId"   : "artist_id"
          },
          "joinType" : "inner"
        },{
          "left" : {
            "tableAlias" : "Albums",
            "columnId"   : "id"
          },
          "right" : {
            "tableAlias" : "Tracks",
            "columnId"   : "album_id"
          },
          "joinType" : "inner"
        }
      ]
    }
  ]
}