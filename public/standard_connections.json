        { "id"    : "artists",
          "alias" : "Artists"     },
        { "id"    : "albums",
          "alias" : "Albums"    },
        { "id"    : "tracks",
          "alias" : "Tracks"    }
      ],
      "joins" : [
        {
          "left" : {
            "tableAlias" : "Artists",
